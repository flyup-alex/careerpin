

    <!-- MAIN CONTENT
    ================================================== -->

    <!-- Background Image -->
    <div class="bg-image hidden"></div>


    <!-- Background Colors -->
    <div class="bg-colors" data-colors="#2ECC71,#1ABC9C,#3498DB,#9B59B6,#E67E22,#E74C3C,#34495E"></div>


    <!-- Navigation -->
    <nav class="navbar navbar-default navbar-fixed-top">
      <div class="container">

        <div class="navbar-header" id="navbar-inline">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="#section_welcome">Career-Pin</a>
        </div>

        <div id="navbar" class="collapse navbar-collapse">

          <a href="#section_download" class="btn btn-lg navbar-right navbar-btn">
            <i class="fa fa-sign-in"></i> Zarejestruj się!
          </a>
          <ul class="nav navbar-nav navbar-right menu">
            <li data-menuanchor="section_welcome">
              <a href="#section_welcome">Witamy!</a>
            </li>
            <li data-menuanchor="section_responsive">
              <a href="#section_responsive">O nas</a>
            </li>
            <li data-menuanchor="section_social">
              <a href="#section_social">Integracja</a>
            </li>
            <li data-menuanchor="section_implementation">
              <a href="#section_implementation">Implementacja</a>
            </li>
            <li data-menuanchor="section_post">
              <a href="#section_post">Personalizacja</a>
            </li>
            <li data-menuanchor="section_pricing">
              <a href="#section_pricing">Cennik</a>
            </li>
            <li data-menuanchor="section_contact-us">
              <a href="#section_contact-us">Kontakt</a>
            </li>
          </ul>
          
        </div><!-- /.navbar-collapse -->

      </div><!-- /.container -->
    </nav>


    <!-- Side menu -->
    <div class="side-menu hidden-xs hidden-sm">
      <ul class="menu">
        <li data-menuanchor="section_welcome">
          <a href="#section_welcome"></a>
        </li>
        <li data-menuanchor="section_responsive">
          <a href="#section_responsive"></a>
        </li>
        <li data-menuanchor="section_social">
          <a href="#section_social"></a>
        </li>
        <li data-menuanchor="section_implementation">
          <a href="#section_implementation"></a>
        </li>
        <li data-menuanchor="section_post">
          <a href="#section_post"></a>
        </li>
        <li data-menuanchor="section_pricing">
          <a href="#section_pricing"></a>
        </li>
        <li data-menuanchor="section_contact-us">
          <a href="#section_contact-us"></a>
        </li>
      </ul>
    </div>


    <!-- Social links -->
    <div class="social-links hidden">
      <ul>
        <li>
          <a href="#"><i class="fa fa-twitter"></i></a>
        </li>
        <li>
          <a href="#"><i class="fa fa-facebook"></i></a>
        </li>
        <li>
          <a href="#"><i class="fa fa-pinterest-p"></i></a>
        </li>
        <li>
          <a href="#"><i class="fa fa-dribbble"></i></a>
        </li>
      </ul>
    </div>


    <!-- Devices -->
    <div class="device__container hidden-xs">

      <!-- iPhone -->
      <div class="iphone__container black hidden">

        <!-- Slides -->
        <div class="iphone__screen">
          <div class="device__slides">
            
            <img src="assets/img/app-screenshots/quest_1.png" alt="...">
            <img src="assets/img/app-screenshots/quest_2.png" alt="...">
            <img src="assets/img/app-screenshots/quest_3.png" alt="...">
            <img src="assets/img/app-screenshots/quest_4.png" alt="...">
            <img src="assets/img/app-screenshots/quest_5.png" alt="...">
            <img src="assets/img/app-screenshots/quest_2.png" alt="...">
            <img src="assets/img/app-screenshots/quest_1.png" alt="...">

          </div>
        </div>

      </div>

      <!-- Samsung -->
      <div class="samsung__container black hidden">

        <!-- Slides -->
        <div class="samsung__screen">
          <div class="device__slides">
            
            <img src="assets/img/app-screenshots/quest_1.png" alt="...">
            <img src="assets/img/app-screenshots/quest_2.png" alt="...">
            <img src="assets/img/app-screenshots/quest_3.png" alt="...">
            <img src="assets/img/app-screenshots/quest_4.png" alt="...">
            <img src="assets/img/app-screenshots/quest_5.png" alt="...">
            <img src="assets/img/app-screenshots/quest_2.png" alt="...">
            <img src="assets/img/app-screenshots/quest_1.png" alt="...">

          </div>
        </div>

      </div>

      <!-- Macbook -->
      <div class="macbook__container">

        <!-- Slides -->
        <div class="macbook__screen">
          <div class="device__slides">
            
                        <%= image_tag("screen_1.jpg") %>
            <%= image_tag("Screenshot_5.jpg") %>
            <%= image_tag("screen_3.jpg") %>
            <%= image_tag("screen_4.jpg") %>
            <%= image_tag("screen_5.jpg") %>
            <%= image_tag("screen_6.jpg") %>
            <%= image_tag("screen_7.jpg") %>
            <%= image_tag("Screenshot_2.jpg") %>

          </div>
        </div>

      </div>

    </div> <!-- / .device__container -->


    <!-- Sections -->
    <div id="fullpage">

      <!-- 1. Welcome -->
      <section data-anchor="section_welcome">
        <div class="container">
          <div class="row">
            <div class="col-sm-6 col-sm-offset-6 col-md-5 col-md-offset-6">
              
              <h1 class="heading" data-animate-in="animateUp">
                <br>
                Zostałoe<span class="free-number"> <%= 80 - User.all.size %></span> z <span class="free-number">80</span> kont próbnych. 
              </h1>

              <p class="delay_1" data-animate-in="animateUp">
                Wyjaśnienie co to w ogóle jest!!!
              </p>
              
              <p class="delay_2 text-center" data-animate-in="animateUp">
                <a href="#section_pricing" class="btn btn-default btn-lg btn-hollow">Przejdź do oferty</a>
              </p>


            </div>
          </div> <!-- / .row -->
        </div><!-- / .container -->
      </section>
      
      <!-- 2. Responsive -->
      <section data-anchor="section_responsive">
        <div class="container">
          <div class="row">
            <div class="col-sm-6 col-sm-offset-6 col-md-5 col-md-offset-6">
                       


    <h2 class="heading" data-animate-in="animateUp">Log in</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field delay_1" data-animate-in="animateUp">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true %>
  </div>

  <div class="field delay_2" data-animate-in="animateUp">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field delay_3" data-animate-in="animateUp">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions delay_4" data-animate-in="animateUp">
    <%= f.submit "Log in" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>




            </div>
          </div> <!-- / .row -->
        </div> <!-- / .container -->
      </section>
      <!-- 3. Social -->
      <section data-anchor="section_social">
        <div class="container">
          <div class="row">
            <div class="col-sm-6 col-sm-offset-6 col-md-5 col-md-offset-6">
              
              <h1 class="heading" data-animate-in="animateUp">
                Tu napiszemy że możemy<br />
                dowolny profil obserwować
              </h1>

              <p class="delay_1" data-animate-in="animateUp">
                Możemy z tego profilu dodawać dowolnie posty na swojego CareerPin.
              </p>
              
              <div class="col-sm-offset-2 col-sm-8 text-center">
                <p class="delay_2" data-animate-in="animateUp">
                  <i class="fa fa-facebook-official"></i> Facebook
                  <br>
                  <i class="fa fa-twitter"></i> Twitter
<!--
                  <br>
                  <i class="fa fa-instagram"></i> Instagram
-->
                </p>
              </div>

            </div>
          </div> <!-- / .row -->
        </div><!-- / .container -->
      </section>

      <!-- 4. Simplycity -->
      <section data-anchor="section_implementation">
        <div class="container">
          <div class="row">
            <div class="col-sm-6 col-sm-offset-6 col-md-5 col-md-offset-6">
                  
              <h2 class="heading" data-animate-in="animateUp">
                Bardzo łatwa<br />
                i szybka implementacja
              </h2>
              
              <p class="delay_1" data-animate-in="animateUp">
                Wystarczy skopiować link wygenerowany przez CareerPin i umieścić go w wybranym miejscu swojej strony.
              </p>
              
              <p class="delay_2" data-animate-in="animateUp">
                Pierwszy krok.
              </p>
              
              <p class="delay_3" data-animate-in="animateUp">
                Drugi krok.
              </p>
              
              <p class="delay_4" data-animate-in="animateUp">
                Trzeci krok.
              </p>


            </div>
          </div> <!-- / .row -->
        </div> <!-- / .container -->
      </section>

      <!-- 5. Own posts -->
      <section data-anchor="section_post">
        <div class="container">
          <div class="row">
            <div class="col-sm-6 col-sm-offset-6 col-md-5 col-md-offset-6">
                  
              <h2 class="heading" data-animate-in="animateUp">
                Możliwość dodania <br>
                własnych postów
              </h2>
              <p class="delay_1" data-animate-in="animateUp">
                Stwórz specjalnie przygotowane posty dla swoich pracowników.
              </p>


            </div>
          </div> <!-- / .row -->
        </div> <!-- / .container -->
      </section>
      
      <!-- 6. Pricing -->
      <section data-anchor="section_pricing">
        <div class="container">
          <div class="row">
            <div class="col-sm-6 col-sm-offset-6 col-md-6 col-md-offset-5">
                     
              <div class="col-xs-6">
                <div class="plan">
                  <ul class="pricing-table list-unstyled" data-animate-in="animateUp">
                      <li class="title" data-animate-in="animateUp">Regular</li>
                      <li class="price" data-animate-in="animateUp">49.99<sup>$</sup> / Monthly</li>
                      <li class="description delay_1" data-animate-in="animateUp">Lorem</li>
                      <li class="description delay_1" data-animate-in="animateUp">Ipsum</li>
                      <li class="description delay_1" data-animate-in="animateUp">Dolor</li>
                      <li class="cta-button delay_2" data-animate-in="animateUp"><a class="btn btn-warning btn-block" href="#">Buy Now</a></li>
                  </ul>
                </div>
              </div>
              
              <div class="col-xs-6">
                <div class="plan">
                  <ul class="pricing-table list-unstyled" data-animate-in="animateUp">
                      <li class="title" data-animate-in="animateUp">Premium</li>
                      <li class="price" data-animate-in="animateUp">74.99<sup>$</sup> / Monthly</li>
                      <li class="description delay_1" data-animate-in="animateUp">Lorem</li>
                      <li class="description delay_1" data-animate-in="animateUp">Ipsum</li>
                      <li class="description delay_1" data-animate-in="animateUp">Dolor</li>
                      <li class="cta-button delay_2" data-animate-in="animateUp"><a class="btn btn-warning btn-block" href="#">Buy Now</a></li>
                  </ul>
                </div>
              </div>

            </div>
          </div> <!-- / .row -->
        </div> <!-- / .container -->
      </section>

      <!-- 7. Contact -->
      <section data-anchor="section_contact-us">
        <div class="container">
          <div class="row">
            <div class="col-sm-6 col-sm-offset-6 col-md-5 col-md-offset-6 contact" data-animate-in="animateUp">

              <h2 class="heading" data-animate-in="animateUp">
                Contact us <br />
                for more information
              </h2>
              
              <h4 class="delay_1" data-animate-in="animateUp">Aleksander Nowak</h4>
              <p class="delay_2" data-animate-in="animateUp">
                <i class="fa fa-mobile"></i>+48796798700
                <br>
                <i class="fa fa-envelope-o"></i><a href="mailto:a.nowak@career-pin.com">a.nowak@career-pin.com</a>
              </p>

            

            </div><!-- /.col-sm-6 col-sm-offset-6 col-md-5 col-md-offset-6 -->
          </div> <!-- / .row -->
        </div> <!-- / .container -->
      </section>

    </div> <!-- / #fullpage -->
  

